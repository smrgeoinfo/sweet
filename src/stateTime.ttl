sostti:Age
  a owl:Class ;
  rdfs:label "age"@en ;
  rdfs:subClassOf sostti:GeologicTimeUnit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorelt:temporalPartOf ;
      owl:someValuesFrom sostti:Epoch ;
    ] ;
.
