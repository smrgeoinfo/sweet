sohu:HumanNeed
  a owl:Class ;
.
<http://sweetontology.net/matrWater>
  a owl:Ontology ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  rdfs:label "SWEET Ontology Material Water" ;
  owl:versionInfo "3.5.0" ;
.
somaw:BlackWater
  a owl:Class ;
  rdfs:label "black water"@en ;
  rdfs:subClassOf somaw:WaterMixture ;
.
somaw:BrackishWater
  a owl:Class ;
  rdfs:label "brackish water"@en ;
  rdfs:subClassOf somaw:SaltWater ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostc:Brackish ;
      owl:onProperty sorel:hasState ;
    ] ;
.
somaw:Brine
  a owl:Class ;
  rdfs:label "brine"@en ;
  rdfs:subClassOf somaw:SaltWater ;
  owl:disjointWith somaw:SalineWater ;
.
somaw:CloudDrop
  a owl:Class ;
  rdfs:label "cloud drop"@en ;
  rdfs:subClassOf somaw:Drop ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue [
          a sopropsdis:SizeRange_micron ;
        ] ;
      owl:onProperty sorelsp:diameter ;
    ] ;
.
somaw:DepthHoar
  a owl:Class ;
  rdfs:label "depth hoar"@en ;
  rdfs:subClassOf somaw:IceCrystal ;
.
somaw:Dew
  a owl:Class ;
  rdfs:label "dew"@en ;
  rdfs:subClassOf somaw:LiquidWater ;
.
somaw:DrinkingWater
  a owl:Class ;
  rdfs:label "drinking water"@en ;
  rdfs:subClassOf sohu:HumanNeed ;
  rdfs:subClassOf somaw:WaterMixture ;
.
somaw:DrizzleDrop
  a owl:Class ;
  rdfs:label "drizzle drop"@en ;
  rdfs:subClassOf somaw:Drop ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue [
          a sopropsdis:SizeRange_micron ;
        ] ;
      owl:onProperty sorelsp:diameter ;
    ] ;
.
somaw:Drop
  a owl:Class ;
  rdfs:label "drop"@en ;
  rdfs:subClassOf somaw:LiquidWater ;
.
somaw:Effluent
  a owl:Class ;
  rdfs:label "effluent"@en ;
  rdfs:subClassOf somaw:WaterMixture ;
.
somaw:Firn
  a owl:Class ;
  rdfs:label "firn"@en ;
  rdfs:subClassOf somaw:IceCrystal ;
.
somaw:FreshWater
  a owl:Class ;
  rdfs:label "fresh water"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  owl:disjointWith somaw:SaltWater ;
.
somaw:Frost
  a owl:Class ;
  rdfs:label "frost"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostp:Frozen ;
      owl:onProperty sorel:hasState ;
    ] ;
.
somaw:GranularIce
  a owl:Class ;
  rdfs:label "granular ice"@en ;
  rdfs:subClassOf somaw:Ice ;
.
somaw:GrayWater
  a owl:Class ;
  rdfs:label "gray water"@en ;
  rdfs:subClassOf somaw:WaterMixture ;
.
somaw:Groundwater
  a owl:Class ;
  rdfs:label "groundwater"@en ;
  rdfs:subClassOf somaw:LiquidWater ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sorea:PlanetarySurface ;
      owl:onProperty sorelsp:below ;
    ] ;
.
somaw:HoarCrystal
  a owl:Class ;
  rdfs:label "hoar crystal"@en ;
  rdfs:subClassOf somaw:IceCrystal ;
.
somaw:Hydrate
  a owl:Class ;
  rdfs:label "hydrate"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
.
somaw:Ice
  a owl:Class ;
  rdfs:label "ice"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostp:Solid ;
      owl:onProperty sorel:hasState ;
    ] ;
.
somaw:IceCrystal
  a owl:Class ;
  rdfs:label "ice crystal"@en ;
  rdfs:subClassOf somaw:Ice ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostso:Crystal ;
      owl:onProperty sorel:hasState ;
    ] ;
  owl:equivalentClass somaw:IceParticle ;
.
somaw:IceParticle
  a owl:Class ;
  rdfs:label "ice particle"@en ;
.
somaw:LiquidWater
  a owl:Class ;
  rdfs:label "liquid water"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostp:Liquid ;
      owl:onProperty sorel:hasState ;
    ] ;
.
somaw:RainDrop
  a owl:Class ;
  rdfs:label "rain drop"@en ;
  rdfs:subClassOf somaw:Drop ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue [
          a sopropsdis:SizeRange_micron ;
        ] ;
      owl:onProperty sorelsp:diameter ;
    ] ;
.
somaw:Rainwater
  a owl:Class ;
  rdfs:label "rainwater"@en ;
  rdfs:subClassOf somaw:LiquidWater ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sophatmopc:Rainfall ;
      owl:onProperty sorelpr:hasSource ;
    ] ;
.
somaw:Rime
  a owl:Class ;
  rdfs:label "rime"@en ;
  rdfs:subClassOf somaw:Ice ;
.
somaw:SalineWater
  a owl:Class ;
  rdfs:label "saline water"@en ;
  rdfs:subClassOf somaw:SaltWater ;
.
somaw:SaltWater
  a owl:Class ;
  rdfs:label "salt water"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostc:Saline ;
      owl:onProperty sorel:hasState ;
    ] ;
.
somaw:SeaWater
  a owl:Class ;
  rdfs:label "sea water"@en ;
  rdfs:subClassOf somaw:SaltWater ;
.
somaw:Sewage
  a owl:Class ;
  rdfs:label "sewage"@en ;
  rdfs:subClassOf somaw:Effluent ;
.
somaw:Snow
  a owl:Class ;
  rdfs:label "snow"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostp:Solid ;
      owl:onProperty sorel:hasState ;
    ] ;
.
somaw:SnowFacies
  a owl:Class ;
  rdfs:label "snow facies"@en ;
  rdfs:subClassOf somaw:IceCrystal ;
.
somaw:SnowGrain
  a owl:Class ;
  rdfs:label "snow grain"@en ;
  rdfs:subClassOf somaw:Snow ;
.
somaw:Steam
  a owl:Class ;
  rdfs:label "steam"@en ;
  owl:equivalentClass somaw:WaterVapor ;
.
somaw:Wastewater
  a owl:Class ;
  rdfs:label "wastewater"@en ;
  rdfs:subClassOf somaw:Effluent ;
.
somaw:WaterMixture
  a owl:Class ;
  rdfs:label "water mixture"@en ;
  rdfs:subClassOf soma:Mixture ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue somac:H2O ;
      owl:onProperty sorelch:hasChemical_1 ;
    ] ;
.
somaw:WaterSubstance
  a owl:Class ;
  rdfs:label "water substance"@en ;
  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Amount_Of_Matter> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue somac:Water ;
      owl:onProperty sorelch:hasChemical ;
    ] ;
.
somaw:WaterVapor
  a owl:Class ;
  rdfs:label "water vapor"@en ;
  rdfs:subClassOf somaw:WaterSubstance ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue sostp:Gas ;
      owl:onProperty sorel:hasState ;
    ] ;
.
sophatmopc:Rainfall
  a owl:Class ;
.
sopropsdis:SizeRange_micron
  a owl:Class ;
.
sorea:PlanetarySurface
  a owl:Class ;
.
sorelch:hasChemical
  a owl:ObjectProperty ;
.
sorelch:hasChemical_1
  a owl:ObjectProperty ;
.
sorelpr:hasSource
  a owl:ObjectProperty ;
.
sorelsp:below
  a owl:ObjectProperty ;
.
sorelsp:diameter
  a owl:ObjectProperty ;
.
sostc:Brackish
  a owl:NamedIndividual ;
.
sostc:Saline
  a owl:NamedIndividual ;
.
sostp:Frozen
  a owl:NamedIndividual ;
.
sostp:Liquid
  a owl:NamedIndividual ;
.
sostso:Crystal
  a owl:NamedIndividual ;
.
