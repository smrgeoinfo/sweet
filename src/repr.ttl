# baseURI: http://sweetontology.net/repr

@prefix : <http://sweetontology.net/repr/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sorelm: <http://sweetontology.net/relaMath/> .
@prefix sorep: <http://sweetontology.net/repr/> .
@prefix sorepm: <http://sweetontology.net/reprMath/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sorelm:eachElementHasType
  a owl:ObjectProperty ;
.
sorelm:hasDimension
  a owl:DatatypeProperty ;
.
<http://sweetontology.net/repr>
  a owl:Ontology ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  rdfs:label "SWEET Ontology Representation" ;
  owl:versionInfo "3.5.0" ;
.
<http://sweetontology.net/repr/1D>
  rdfs:label "1d"@en ;
.
<http://sweetontology.net/repr/2D>
  rdfs:label "2d"@en ;
.
<http://sweetontology.net/repr/3D>
  rdfs:label "3d"@en ;
.
sorep:Barycentric
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "barycentric"@en ;
.
sorep:CardinalScale
  a owl:Class ;
  rdfs:label "cardinal scale"@en ;
  rdfs:subClassOf sorep:QuantitativeScale ;
.
sorep:CategoricalScale
  a owl:Class ;
  rdfs:label "categorical scale"@en ;
  rdfs:subClassOf sorep:MeasurementScale ;
  owl:disjointWith sorep:OrdinalScale ;
  owl:disjointWith sorep:QuantitativeScale ;
.
sorep:Component
  a owl:Class ;
  rdfs:label "component"@en ;
  owl:equivalentClass sorep:Coordinate ;
.
sorep:Coordinate
  a owl:Class ;
  rdfs:label "coordinate"@en ;
  rdfs:subClassOf sorepm:Scalar ;
.
sorep:CoordinateSystem
  a owl:Class ;
  rdfs:label "coordinate system"@en ;
  rdfs:subClassOf sorep:ReferenceFrame ;
.
sorep:Coordinates
  a owl:Class ;
  rdfs:label "coordinates"@en ;
  rdfs:subClassOf sorepm:Array ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sorep:Coordinate ;
      owl:onProperty sorelm:eachElementHasType ;
    ] ;
.
sorep:D
  a owl:Class ;
  rdfs:subClassOf sorep:Representation ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue 1 ;
      owl:onProperty sorelm:hasDimension ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue 2 ;
      owl:onProperty sorelm:hasDimension ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue 3 ;
      owl:onProperty sorelm:hasDimension ;
    ] ;
.
sorep:DependentVariable
  a owl:Class ;
  rdfs:label "dependent variable"@en ;
  rdfs:subClassOf sorepm:Variable ;
.
sorep:Dimension
  a owl:Class ;
  rdfs:label "dimension"@en ;
  rdfs:subClassOf sorep:Representation ;
.
sorep:Eulerian
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "eulerian"@en ;
.
sorep:Geocentric
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "geocentric"@en ;
.
sorep:Geodetic
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "geodetic"@en ;
.
sorep:Heliocentric
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "heliocentric"@en ;
.
sorep:IndependentVariable
  a owl:Class ;
  rdfs:label "independent variable"@en ;
  rdfs:subClassOf sorepm:Variable ;
.
sorep:IntervalScale
  a owl:Class ;
  rdfs:label "interval scale"@en ;
  rdfs:subClassOf sorep:QuantitativeScale ;
.
sorep:Knowledge
  a owl:Class ;
  rdfs:label "knowledge"@en ;
  rdfs:subClassOf sorep:Representation ;
.
sorep:Lagrangian
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "lagrangian"@en ;
.
sorep:LogarithmicScale
  a owl:Class ;
  rdfs:label "logarithmic scale"@en ;
  rdfs:subClassOf sorep:QuantitativeScale ;
.
sorep:Measurement
  a owl:Class ;
  rdfs:label "measurement"@en ;
  rdfs:subClassOf sorep:Representation ;
.
sorep:MeasurementScale
  a owl:Class ;
  rdfs:label "measurement scale"@en ;
  owl:equivalentClass sorep:Scale ;
.
sorep:OrdinalScale
  a owl:Class ;
  rdfs:label "ordinal scale"@en ;
  rdfs:subClassOf sorep:MeasurementScale ;
  owl:disjointWith sorep:QuantitativeScale ;
.
sorep:Qualitative
  a owl:Class ;
  rdfs:label "qualitative"@en ;
  rdfs:subClassOf sorep:MeasurementScale ;
.
sorep:QuantitativeScale
  a owl:Class ;
  rdfs:label "quantitative scale"@en ;
  rdfs:subClassOf sorep:MeasurementScale ;
.
sorep:RatioScale
  a owl:Class ;
  rdfs:label "ratio scale"@en ;
  rdfs:subClassOf sorep:MeasurementScale ;
.
sorep:ReferenceFrame
  a owl:Class ;
  rdfs:label "reference frame"@en ;
  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Property> ;
.
sorep:Representation
  a owl:Class ;
  rdfs:label "representation"@en ;
.
sorep:Scale
  a owl:Class ;
  rdfs:label "scale"@en ;
  rdfs:subClassOf sorep:Representation ;
.
sorep:Spectral
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "spectral"@en ;
.
sorep:Synoptic
  a sorep:ReferenceFrame ;
  a owl:NamedIndividual ;
  rdfs:label "synoptic"@en ;
.
sorepm:Array
  a owl:Class ;
.
sorepm:NumericalEntity
  a owl:Class ;
.
sorepm:Scalar
  a owl:Class ;
.
sorepm:Variable
  a owl:Class ;
.
[
  a owl:AllDifferent ;
  owl:distinctMembers (
      sorep:Eulerian
      sorep:Lagrangian
    ) ;
].
