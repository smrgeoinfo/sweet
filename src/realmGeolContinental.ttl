<http://sweetontology.net/realmGeolContinental>
  a owl:Ontology ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  rdfs:label "SWEET Ontology Realm Geologic Continental" ;
  owl:versionInfo "3.5.0" ;
.
soreagcont:ActiveContinent
  a owl:Class ;
  rdfs:label "active continent"@en ;
  rdfs:subClassOf soreag:GeologicProvince ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:ContinentalLithosphere ;
      owl:onProperty sorel:partOf ;
    ] ;
.
soreagcont:ContinentalCrust
  a owl:Class ;
  rdfs:label "continental crust"@en ;
  rdfs:subClassOf soreag:Continental ;
  rdfs:subClassOf soreag:Crust ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:ContinentalLithosphere ;
      owl:onProperty sorel:partOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorelsc:hasRock ;
      owl:someValuesFrom matrRockIgneous:Felsic ;
    ] ;
.
soreagcont:ContinentalLithosphere
  a owl:Class ;
  rdfs:label "continental lithosphere"@en ;
  rdfs:subClassOf soreag:Continental ;
  rdfs:subClassOf soreag:Lithosphere ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:Craton ;
      owl:onProperty sorelph:hasPlanetaryStructure ;
    ] ;
.
soreagcont:ContinentalMargin
  a owl:Class ;
  rdfs:label "continental margin"@en ;
.
soreagcont:ContinentalPlatform
  a owl:Class ;
  rdfs:label "continental platform"@en ;
  rdfs:subClassOf soreag:GeologicProvince ;
.
soreagcont:ContinentalRift
  a owl:Class ;
  rdfs:label "continental rift"@en ;
  rdfs:subClassOf sorealt:DivergentBoundary ;
  rdfs:subClassOf sorealt:Rift ;
.
soreagcont:Craton
  a owl:Class ;
  dcterms:contributor <http://linked.data.gov.au/org/gsq> ;
  dcterms:source <http://www.glossaryofgeology.org> ;
  rdfs:subClassOf soreag:GeologicProvince ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom matrRockIgneous:Kimberlite ;
      owl:onProperty sorelch:hasSubstance ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagb:IntracratonicBasin ;
      owl:onProperty sorelph:hasPlanetaryStructure ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:ContinentalCrust ;
      owl:onProperty sorel:partOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:ContinentalPlatform ;
      owl:onProperty sorelph:hasPlanetaryStructure ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:Shield ;
      owl:onProperty sorelph:hasPlanetaryStructure ;
    ] ;
  skos:definition "A craton is part of Earth's continental crust that has attained stability and has been little deformed for a prolonged period. Cratons include shield areas, where Precambrian rocks are exposed, and platform areas, where Precambrian rocks are overlain by a thin layer of Phanerozoic strata. (Neuendorf et al., 2011)"@en ;
  skos:prefLabel "craton"@en ;
.
soreagcont:Shield
  a owl:Class ;
  dcterms:contributor <http://linked.data.gov.au/org/gsq> ;
  dcterms:source <http://www.glossaryofgeology.org> ;
  rdfs:label "shield"@en ;
  rdfs:subClassOf soreag:GeologicProvince ;
  skos:definition "A large area of exposed basement rock in a craton, commonly with a very gently convex surface, surrounded by sediment covered platforms. The rocks of virtually all shield areas are Precambrian. (Neuendorf et al., 2011)"@en ;
.
soreagcont:StableContinent
  a owl:Class ;
  rdfs:label "stable continent"@en ;
  rdfs:subClassOf soreag:GeologicProvince ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:ContinentalLithosphere ;
      owl:onProperty sorel:partOf ;
    ] ;
.
soreagcont:Supercontinent
  a owl:Class ;
  rdfs:label "supercontinent"@en ;
  rdfs:subClassOf soreag:GeologicProvince ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:ContinentalLithosphere ;
      owl:onProperty sorel:hasRealm ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:Craton ;
      owl:onProperty sorelph:hasPlanetaryStructure ;
    ] ;
.
soreagcont:Supercraton
  a owl:Class ;
  dcterms:source <http://doi.org/10.22459/SN.08.2012> ;
  rdfs:subClassOf soreag:GeologicProvince ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreagcont:Craton ;
      owl:onProperty sorel:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "2"^^xsd:int ;
      owl:onProperty sorel:hasPart ;
    ] ;
  skos:definition "A large, ancestral (largely Archean) landmass consisting of two or more cratons. (Blewett, 2012)"@en ;
  skos:prefLabel "supercraton"@en ;
.
