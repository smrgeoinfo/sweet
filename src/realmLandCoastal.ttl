# baseURI: http://sweetontology.net/realmLandCoastal

@prefix : <http://sweetontology.net/realmLandCoastal/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sophoc: <http://sweetontology.net/phenOceanCoastal/> .
@prefix sorea: <http://sweetontology.net/realm/> .
@prefix soreahb: <http://sweetontology.net/realmHydroBody/> .
@prefix soreal: <http://sweetontology.net/realmLandform/> .
@prefix sorealc: <http://sweetontology.net/realmLandCoastal/> .
@prefix soreao: <http://sweetontology.net/realmOcean/> .
@prefix sorel: <http://sweetontology.net/rela/> .
@prefix sorelsp: <http://sweetontology.net/relaSpace/> .
@prefix sostrg: <http://sweetontology.net/stateRoleGeographic/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://sweetontology.net/Arch>
  a owl:Class ;
.
<http://sweetontology.net/realmLandCoastal>
  a owl:Ontology ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  rdfs:label "SWEET Ontology Realm Land Coastal" ;
  owl:versionInfo "3.5.0" ;
.
sorealc:Atoll
  a owl:Class ;
  rdfs:label "atoll"@en ;
  rdfs:subClassOf sorealc:Island ;
.
sorealc:Bar
  a owl:Class ;
  rdfs:label "bar"@en ;
  rdfs:subClassOf sorealc:Delta ;
  rdfs:subClassOf sorealc:Spit ;
  owl:equivalentClass sorealc:DeltaBar ;
.
sorealc:BarrierIsland
  a owl:Class ;
  rdfs:label "barrier island"@en ;
  rdfs:subClassOf sorealc:Island ;
.
sorealc:Beach
  a owl:Class ;
  rdfs:label "beach"@en ;
  rdfs:subClassOf sorealc:CoastalLandform ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreahb:BodyOfWater ;
      owl:onProperty sorelsp:isAdjacentTo ;
    ] ;
.
sorealc:Coastal
  a owl:Class ;
  rdfs:label "coastal"@en ;
  owl:equivalentClass sorealc:CoastalRegion ;
.
sorealc:CoastalLandform
  a owl:Class ;
  rdfs:label "coastal landform"@en ;
  rdfs:subClassOf soreal:Landform ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorel:hasRealm ;
      owl:someValuesFrom sorealc:CoastalRegion ;
    ] ;
.
sorealc:CoastalMargin
  a owl:Class ;
  rdfs:label "coastal margin"@en ;
  rdfs:subClassOf <http://sweetontology.net/realmGeol/TectonicFeature> ;
.
sorealc:CoastalPlain
  a owl:Class ;
  rdfs:label "coastal plain"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
  rdfs:subClassOf soreal:Plain ;
.
sorealc:CoastalRegion
  a owl:Class ;
  rdfs:label "coastal region"@en ;
  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Geologic_Setting> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorelsp:hasCommon2DBorderWith ;
      owl:someValuesFrom sorea:Land ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorelsp:hasCommon2DBorderWith ;
      owl:someValuesFrom soreao:OceanRegion ;
    ] ;
.
sorealc:Cove
  a owl:Class ;
  rdfs:label "cove"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:Delta
  a owl:Class ;
  rdfs:label "delta"@en ;
  rdfs:subClassOf sorealc:CoastalLandform ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorel:hasRealm ;
      owl:someValuesFrom soreahb:River ;
    ] ;
.
sorealc:DeltaBar
  a owl:Class ;
  rdfs:label "delta bar"@en ;
.
sorealc:Harbor
  a owl:Class ;
  rdfs:label "harbor"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:Hinterland
  a owl:Class ;
  rdfs:label "hinterland"@en ;
  rdfs:subClassOf soreal:LandRegion ;
.
sorealc:Intertidal
  a owl:Class ;
  rdfs:label "intertidal"@en ;
  owl:equivalentClass sorealc:IntertidalZone ;
.
sorealc:IntertidalZone
  a owl:Class ;
  rdfs:label "intertidal zone"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sophoc:OceanTide ;
      owl:onProperty sorel:hasPhenomena ;
    ] ;
  owl:equivalentClass sorealc:Littoral ;
  owl:equivalentClass sorealc:LittoralZone ;
  owl:equivalentClass sorealc:TidePool ;
.
sorealc:Island
  a owl:Class ;
  rdfs:label "island"@en ;
  rdfs:subClassOf soreal:Landform ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreahb:BodyOfWater ;
      owl:onProperty sorelsp:surroundedBy ;
    ] ;
.
sorealc:IslandArc
  a owl:Class ;
  rdfs:label "island arc"@en ;
  rdfs:subClassOf sorealc:CoastalLandform ;
.
sorealc:Isthmus
  a owl:Class ;
  rdfs:label "isthmus"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:Key
  a owl:Class ;
  rdfs:label "key"@en ;
  rdfs:subClassOf sorealc:Island ;
.
sorealc:Littoral
  a owl:Class ;
  rdfs:label "littoral"@en ;
.
sorealc:LittoralZone
  a owl:Class ;
  rdfs:label "littoral zone"@en ;
.
sorealc:MudFlat
  a owl:Class ;
  rdfs:label "mud flat"@en ;
  rdfs:subClassOf sorealc:CoastalLandform ;
.
sorealc:Riparian
  a owl:Class ;
  rdfs:label "riparian"@en ;
  owl:equivalentClass sorealc:RiparianZone ;
.
sorealc:RiparianZone
  a owl:Class ;
  rdfs:label "riparian zone"@en ;
.
sorealc:Shoal
  a owl:Class ;
  rdfs:label "shoal"@en ;
  rdfs:subClassOf sorealc:CoastalLandform ;
.
sorealc:Shore
  a owl:Class ;
  rdfs:label "shore"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
  rdfs:subClassOf soreal:Land ;
  owl:equivalentClass sorealc:Shoreline ;
.
sorealc:ShorePlatform
  a owl:Class ;
  rdfs:label "shore platform"@en ;
  owl:equivalentClass sorealc:WaveCutPlatform ;
.
sorealc:Shoreline
  a owl:Class ;
  rdfs:label "shoreline"@en ;
.
sorealc:Spit
  a owl:Class ;
  rdfs:label "spit"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:Supratidal
  a owl:Class ;
  rdfs:label "supratidal"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:Terracette
  a owl:Class ;
  rdfs:label "terracette"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:Tidal
  a owl:Class ;
  rdfs:label "tidal"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:TidePool
  a owl:Class ;
  rdfs:label "tide pool"@en ;
.
sorealc:Tombolo
  a owl:Class ;
  rdfs:label "tombolo"@en ;
  rdfs:subClassOf sorealc:Bar ;
.
sorealc:Updrift
  a owl:Class ;
  rdfs:label "updrift"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:WaveAttackZone
  a owl:Class ;
  rdfs:label "wave attack zone"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
sorealc:WaveCutPlatform
  a owl:Class ;
  rdfs:label "wave cut platform"@en ;
  rdfs:subClassOf sorealc:CoastalRegion ;
.
soreal:Land
  a owl:Class ;
.
soreal:LandRegion
  a owl:Class ;
.
soreal:Landform
  a owl:Class ;
.
soreal:Plain
  a owl:Class ;
.
soreao:OceanRegion
  a owl:Class ;
.
sorel:hasPhenomena
  a owl:ObjectProperty ;
.
sorel:hasRealm
  a owl:ObjectProperty ;
.
sorelsp:hasCommon2DBorderWith
  a owl:ObjectProperty ;
.
sorelsp:isAdjacentTo
  a owl:ObjectProperty ;
.
sorelsp:surroundedBy
  a owl:ObjectProperty ;
.
sostrg:Margin
  a owl:Class ;
.
