soreas:A_Horizon
  a owl:Class ;
  rdfs:label "a horizon"@en ;
  rdfs:subClassOf soreas:SoilLayer ;
  owl:equivalentClass soreas:Topsoil ;
.
soreas:ActiveLayer
  a owl:Class ;
  rdfs:label "active layer"@en ;
  rdfs:subClassOf soreas:SoilLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty sorel:partOf ;
      owl:someValuesFrom soreas:Permafrost ;
    ] ;
.
