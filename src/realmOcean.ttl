<http://sweetontology.net/realmOcean>
  a owl:Ontology ;
  dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  rdfs:label "SWEET Ontology Realm Ocean" ;
  owl:versionInfo "3.5.0" ;
.
soreao:Abyss
  a owl:Class ;
  rdfs:label "abyss"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  owl:equivalentClass soreao:Abyssal ;
.
soreao:Abyssal
  a owl:Class ;
  rdfs:label "abyssal"@en ;
.
soreao:AbyssopelagicZone
  a owl:Class ;
  rdfs:label "abyssopelagic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:AphoticZone
  a owl:Class ;
  rdfs:label "aphotic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sopropi:Turbidity ;
      owl:onProperty sorelsc:hasQuantity ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:PhoticZone ;
      owl:onProperty sorelsp:below ;
    ] ;
.
soreao:BarrierLayer
  a owl:Class ;
  rdfs:label "barrier layer"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:Bathyal
  a owl:Class ;
  rdfs:label "bathyal"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:BathypelagicZone
  a owl:Class ;
  rdfs:label "bathypelagic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:BenthicZone
  a owl:Class ;
  rdfs:label "benthic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:DemersalZone ;
      owl:onProperty sorelsp:below ;
    ] ;
.
soreao:DemersalZone
  a owl:Class ;
  rdfs:label "demersal zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:BenthicZone ;
      owl:onProperty sorelsp:above ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:ProfundalZone ;
      owl:onProperty sorel:hasRealm ;
    ] ;
.
soreao:EuphoticZone
  a owl:Class ;
  rdfs:label "euphotic zone"@en ;
  owl:equivalentClass soreao:PhoticZone ;
.
soreao:Hadal
  a owl:Class ;
  rdfs:label "hadal"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:Halocline
  a owl:Class ;
  rdfs:label "halocline"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sopropche:Salinity ;
      owl:onProperty sorelm:hasGradient ;
    ] ;
.
soreao:LimneticZone
  a owl:Class ;
  rdfs:label "limnetic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:ProfundalZone ;
      owl:onProperty sorelsp:above ;
    ] ;
.
soreao:MesopelagicZone
  a owl:Class ;
  rdfs:label "mesopelagic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:MixedLayer
  a owl:Class ;
  rdfs:label "mixed layer"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:NeriticZone
  a owl:Class ;
  rdfs:label "neritic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
.
soreao:OceanRegion
  a owl:Class ;
  rdfs:label "ocean region"@en ;
  rdfs:subClassOf <http://loop3d.org/GSO/ontology/2020/1/common/Spatial_Region> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sorea:Ocean ;
      owl:onProperty sorel:hasRealm ;
    ] ;
.
soreao:OpenOcean
  a owl:Class ;
  rdfs:label "open ocean"@en ;
.
soreao:PelagicSubZone
  a owl:Class ;
  rdfs:label "pelagic sub zone"@en ;
  rdfs:subClassOf sorea:Ocean ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:PelagicZone ;
      owl:onProperty sorel:partOf ;
    ] ;
.
soreao:PelagicZone
  a owl:Class ;
  rdfs:label "pelagic zone"@en ;
.
soreao:PhoticZone
  a owl:Class ;
  rdfs:label "photic zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sophb:Photosynthesis ;
      owl:onProperty sorel:hasPhenomena ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sopropi:Turbidity ;
      owl:onProperty sorelsc:hasQuantity ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:AphoticZone ;
      owl:onProperty sorelsp:above ;
    ] ;
.
soreao:Polynya
  a owl:Class ;
  rdfs:label "polynya"@en ;
  rdfs:subClassOf sorea:Ocean ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreac:SeaIce ;
      owl:onProperty sorelsp:surroundedBy ;
    ] ;
.
soreao:ProfundalZone
  a owl:Class ;
  rdfs:label "profundal zone"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soreao:AphoticZone ;
      owl:onProperty sorel:hasRealm ;
    ] ;
.
soreao:Pycnocline
  a owl:Class ;
  rdfs:label "pycnocline"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sopropche:Salinity ;
      owl:onProperty sorelm:hasGradient ;
    ] ;
.
soreao:Thermocline
  a owl:Class ;
  rdfs:label "thermocline"@en ;
  rdfs:subClassOf soreao:WaterBodyLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom soproptg:TemperatureGradient ;
      owl:onProperty sorelsc:hasQuantity ;
    ] ;
.
soreao:WaterBodyLayer
  a owl:Class ;
  rdfs:label "water body layer"@en ;
  rdfs:subClassOf sorea:PlanetaryLayer ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sorea:Ocean ;
      owl:onProperty sorel:hasRealm ;
    ] ;
.
sorel:hasPhenomena
  a owl:ObjectProperty ;
.
sorel:hasRealm
  a owl:ObjectProperty ;
.
sorel:partOf
  a owl:ObjectProperty ;
.
sorelm:hasGradient
  a owl:ObjectProperty ;
.
sorelsc:hasQuantity
  a owl:ObjectProperty ;
.
