#################################################################
#    Individuals
#################################################################

###  http://sweetontology.net/realm/Earth
sorea:Earth rdf:type owl:NamedIndividual ;
            sorel:hasPart soreaer:EarthAtmosphere ;
            rdfs:label "earth"@en .


###  http://sweetontology.net/realm/EarthGeosphere
sorea:EarthGeosphere rdf:type owl:NamedIndividual .


###  http://sweetontology.net/realmAstroHelio/EarthExosphere
soreaah:EarthExosphere rdf:type owl:NamedIndividual ,
                                soreag:AtmosphereLayer ;
                       sorel:isPartOf soreaer:EarthAtmosphere ;
                       dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                       dcterms:source <https://en.wikipedia.org/wiki/Thermosphere> ;
                       skos:definition "Earth's exoophere is a planetary layer of Earth's atmosphere which is its outermost layer. It extends from the exobase, which is located at the top of the thermosphere at an altitude of about 700 km above sea level, to about 10,000 km (6,200 mi; 33,000,000 ft) where it merges into the solar wind."@en ;
                       skos:prefLabel "earth exosphere"@en .


###  http://sweetontology.net/realmAstroHelio/EarthIonosphere
soreaah:EarthIonosphere rdf:type owl:NamedIndividual ,
                                 soreag:AtmosphereLayer ;
                        sorel:isPartOf soreaer:EarthAtmosphere ;
                        dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                        dcterms:source <https://en.wikipedia.org/wiki/Ionosphere> ;
                        skos:definition "Earth's ionosphere is a planetary layer of Earth's atmosphere which is the ionized part of Earth's upper atmosphere, from about 60 km (37 mi) to 1,000 km (620 mi) in altitude."@en ;
                        skos:prefLabel "earth ionosphere"@en .


###  http://sweetontology.net/realmEarthReference/AbyssopelagicLayer
soreaer:AbyssopelagicLayer rdf:type owl:NamedIndividual ,
                                    soreag:HydrosphereLayer ;
                           sorel:isPartOf soreaer:AphoticLayer ;
                           dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                           dcterms:source <https://en.wikipedia.org/wiki/Abyssal_zone> ;
                           skos:altLabel "abyssal zone" ,
                                         "abyssopelagic zone" ;
                           skos:definition "The abyssopelagic layer is a planetary layer of Earth's oceans between 3,000 and 6,000 metres below the ocean surface that remains in perpetual darkness and where the seawater is devoid of oxygen due to no photosynthetic action."@en ;
                           skos:prefLabel "abyssopelagic layer" .


###  http://sweetontology.net/realmEarthReference/AphoticLayer
soreaer:AphoticLayer rdf:type owl:NamedIndividual ,
                              soreag:HydrosphereLayer ;
                     sorel:hasPart soreaer:AbyssopelagicLayer ,
                                   soreaer:BathypelagicLayer ,
                                   soreaer:HadopelagicLayer ,
                                   soreaer:MesopelagicLayer ;
                     sorel:isPartOf soreaer:PelagicLayer ;
                     dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                     dcterms:source <https://en.wikipedia.org/wiki/Aphotic_zone> ;
                     skos:altLabel "aphotic zone"@en ;
                     skos:definition "The aphotic layer is a planetary layer of Earth's hydrosphere where there is little or no sunlight and thus organisms within it cannot perform photosynthesis."@en ;
                     skos:prefLabel "aphotic layer"@en .


###  http://sweetontology.net/realmEarthReference/Asthenosphere
soreaer:Asthenosphere rdf:type owl:NamedIndividual ,
                               soreag:GeosphereLayer ;
                      sorel:isPartOf soreaer:UpperMantle ;
                      dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                      dcterms:source <https://en.wikipedia.org/wiki/Asthenosphere> ;
                      skos:definition "Earth's asthenosphere is a planetary layer within Earth's upper mantle that is  highly viscous, mechanically weak and a ductile region. It lies below the lithosphere, at depths between approximately 80 and 200 km (50 and 120 miles) below the surface."@en ;
                      skos:prefLabel "asthenosphere"@en .


###  http://sweetontology.net/realmEarthReference/BathypelagicLayer
soreaer:BathypelagicLayer rdf:type owl:NamedIndividual ,
                                   soreag:HydrosphereLayer ;
                          sorel:isPartOf soreaer:AphoticLayer ;
                          dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                          dcterms:source <https://en.wikipedia.org/wiki/Pelagic_zone> ;
                          skos:altLabel "bathypelagic zone"@en ;
                          skos:definition "The bathypelagic layer is a planetary layer of Earth's hydrosphere that extends from a depth of 1,000 to 4,000 m below the ocean hydrosphere surface where sunlight does not reach."@en ;
                          skos:prefLabel "bathypelagic layer"@en .


###  http://sweetontology.net/realmEarthReference/BenthicLayer
soreaer:BenthicLayer rdf:type owl:NamedIndividual ,
                              soreag:HydrosphereLayer ;
                     sorel:isPartOf soreaer:EarthHydrosphere ;
                     dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                     dcterms:source <https://en.wikipedia.org/wiki/Benthic_zone> ;
                     skos:altLabel "benthic zone"@en ;
                     skos:definition "The benthic layer is a planetary layer of Earth's hydrosphere that is the ecological region at the lowest level of a body of water such as an ocean, lake, or stream, including the sediment surface and some sub-surface layers."@en ;
                     skos:prefLabel "benthic layer"@en .


###  http://sweetontology.net/realmEarthReference/BrittleDuctileTransitionZone
soreaer:BrittleDuctileTransitionZone rdf:type owl:NamedIndividual ,
                                              soreag:GeosphereLayer ;
                                     skos:prefLabel "brittle ductile transition zone"@en .


###  http://sweetontology.net/realmEarthReference/Core
soreaer:Core rdf:type owl:NamedIndividual ,
                      soreag:GeosphereLayer ;
             sorel:hasPart soreaer:InnerCore ,
                           soreaer:OuterCore ;
             sorel:isPartOf sorea:EarthGeosphere ;
             dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
             dcterms:source <https://en.wikipedia.org/wiki/Structure_of_the_Earth#Core> ;
             skos:definition "Earth's core is a planetary layer within Earth's geosphere that is its innermost layer. It is immediately beneath the mantle and is generally believed to be composed primarily of iron and some nickel."@en ;
             skos:prefLabel "core"@en .


###  http://sweetontology.net/realmEarthReference/Crust
soreaer:Crust rdf:type owl:NamedIndividual ,
                       soreag:GeosphereLayer ;
              sorel:hasPart soreaer:BrittleDuctileTransitionZone ,
                            soreaer:TransitionalCrust ;
              dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
              dcterms:source <https://en.wikipedia.org/wiki/Earth%27s_crust> ;
              skos:definition "Earth's crust is a planetary layer within Earth's lithosphere that is its outermost layer. The crust lies on top of the mantle, also within the lithosphere, with the boundary between the crust and mantle conventionally placed at the Mohorovičić discontinuity, a boundary defined by a contrast in seismic velocity."@en ;
              skos:prefLabel "crust"@en .


###  http://sweetontology.net/realmEarthReference/DLayer
soreaer:DLayer rdf:type owl:NamedIndividual ,
                        soreag:GeosphereLayer ;
               dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
               skos:prefLabel "d-layer"@en .


###  http://sweetontology.net/realmEarthReference/DemersalLayer
soreaer:DemersalLayer rdf:type owl:NamedIndividual ,
                               soreag:HydrosphereLayer ;
                      sorel:isPartOf soreaer:EarthHydrosphere ;
                      dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                      dcterms:source <https://en.wikipedia.org/wiki/Demersal_zone> ;
                      skos:altLabel "demersal zone"@en ;
                      skos:definition "The demersal layer is a planetary layer of Earth's hydrosphere consisting of the part of the water column near to (and significantly affected by) the seabed or freshwater bottom (lake/river bed)."@en ;
                      skos:prefLabel "demersal layer"@en .


###  http://sweetontology.net/realmEarthReference/EarthAtmosphere
soreaer:EarthAtmosphere rdf:type owl:NamedIndividual ,
                                 sorea:Atmosphere ;
                        sorel:hasPart soreaer:EarthExosphere ,
                                      soreaer:EarthMesosphere ,
                                      soreaer:EarthStratosphere ,
                                      soreaer:EarthThermosphere ,
                                      soreaer:EarthTroposphere ;
                        sorelph:hasAstronomicalBody sorea:Earth ;
                        dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                        dcterms:source <https://en.wikipedia.org/wiki/Atmosphere_of_Earth> ;
                        skos:definition "The atmosphere of Earth is the layer of gases, commonly known as air, that surrounds the planet Earth and is retained by Earth's gravity. (Wikipedia)"@en ;
                        skos:prefLabel "earth atmosphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthExosphere
soreaer:EarthExosphere rdf:type owl:NamedIndividual ;
                       sorel:isPartOf soreaer:EarthAtmosphere ;
                       dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                       dcterms:source <https://en.wikipedia.org/wiki/Exosphere> ;
                       skos:definition "Earth's exosphere is a planetary layer of Earth's atmosphere where molecules are gravitationally bound to that body, but where the density is too low for them to behave as a gas by colliding with each other."@en .


###  http://sweetontology.net/realmEarthReference/EarthGeosphere
soreaer:EarthGeosphere rdf:type owl:NamedIndividual ,
                                sorea:Geosphere ;
                       dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                       skos:prefLabel "earth geosphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthHydrosphere
soreaer:EarthHydrosphere rdf:type owl:NamedIndividual ,
                                  sorea:Hydrosphere ;
                         owl:sameAs soreaer:EarthLiquisphere ;
                         sorel:hasPart soreaer:AbyssopelagicLayer ,
                                       soreaer:AphoticLayer ,
                                       soreaer:BathypelagicLayer ,
                                       soreaer:BenthicLayer ,
                                       soreaer:DemersalLayer ;
                         dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                         dcterms:definition "The combined mass of water found on, under, and above the surface of a planet earth,"@en ;
                         dcterms:source <https://en.wikipedia.org/wiki/Hydrosphere> ;
                         skos:prefLabel "earth hydrosphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthLiquisphere
soreaer:EarthLiquisphere rdf:type owl:NamedIndividual ,
                                  sorea:Liquisphere ;
                         dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                         dcterms:definition "The liquid planetary layer on Earth which is entirely a hydrosphere layer"@en ;
                         skos:prefLabel "earth liquisphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthMesosphere
soreaer:EarthMesosphere rdf:type owl:NamedIndividual ,
                                 soreag:AtmosphereLayer ;
                        sorel:isPartOf soreaer:EarthAtmosphere ;
                        dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                        dcterms:source <https://en.wikipedia.org/wiki/Mesosphere> ;
                        skos:definition "Earth's mesosphere is a planetary layer of Earth's atmosphere which is the third layer of the atmosphere, directly above the stratosphere and directly below the thermosphere. In the mesosphere, temperature decreases as altitude increases."@en ;
                        skos:prefLabel "earth mesosphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthStratosphere
soreaer:EarthStratosphere rdf:type owl:NamedIndividual ,
                                   soreag:AtmosphereLayer ;
                          sorel:isPartOf soreaer:EarthAtmosphere ;
                          dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                          dcterms:source <https://en.wikipedia.org/wiki/Stratosphere> ;
                          skos:definition "Earth's stratosphere is a planetary layer of Earth's atmosphere which is the second layer of the atmosphere, directly above above the troposphere, and below the mesosphere. The stratosphere is stratified (layered) in temperature, with warmer layers higher and cooler layers closer to the Earth."@en ;
                          skos:prefLabel "earth stratosphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthThermosphere
soreaer:EarthThermosphere rdf:type owl:NamedIndividual ,
                                   soreag:AtmosphereLayer ;
                          sorel:isPartOf soreaer:EarthAtmosphere ;
                          dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                          dcterms:source <https://en.wikipedia.org/wiki/Thermosphere> ;
                          skos:definition "Earth's thermophere is a planetary layer of Earth's atmosphere directly above the mesosphere and below the exosphere. Within this layer, ultraviolet radiation causes photoionization/photodissociation of molecules, creating ions in the ionosphere."@en ;
                          skos:prefLabel "earth thermosphere"@en .


###  http://sweetontology.net/realmEarthReference/EarthTroposphere
soreaer:EarthTroposphere rdf:type owl:NamedIndividual ,
                                  soreag:AtmosphereLayer ;
                         sorel:isPartOf soreaer:EarthAtmosphere ;
                         dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                         dcterms:source <https://en.wikipedia.org/wiki/Troposphere> ;
                         skos:definition "Earth's troposphere is a planetary layer of Earth's atmosphere which is the lowest layer of the atmosphere, and is also where nearly all weather conditions take place."@en ;
                         skos:prefLabel "earth troposphere"@en .


###  http://sweetontology.net/realmEarthReference/EpipelagicLayer
soreaer:EpipelagicLayer rdf:type owl:NamedIndividual ,
                                 soreag:HydrosphereLayer ;
                        sorel:isPartOf soreaer:PhoticLayer ;
                        dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                        dcterms:source <https://en.wikipedia.org/wiki/Pelagic_zone#Epipelagic_(sunlight)> ;
                        skos:altLabel "epipelagic zone"@en ;
                        skos:prefLabel "epipelagic layer"@en .


###  http://sweetontology.net/realmEarthReference/HadopelagicLayer
soreaer:HadopelagicLayer rdf:type owl:NamedIndividual ,
                                  soreag:HydrosphereLayer ;
                         sorel:isPartOf soreaer:AphoticLayer ;
                         dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                         dcterms:source <https://en.wikipedia.org/wiki/Hadal_zone> ;
                         skos:altLabel "hadal zone"@en ,
                                       "hadopelagic zone"@en ;
                         skos:definition "The hadopelagic layer is the deepest of the planetary layer of Earth's oceans lying between 6,000 and 11,000 metres below the ocean surfacethat exists in long but narrow topographic V-shaped depressions."@en ;
                         skos:prefLabel "hadoopelagic layer"@en .


###  http://sweetontology.net/realmEarthReference/InnerCore
soreaer:InnerCore rdf:type owl:NamedIndividual ,
                           soreag:GeosphereLayer ;
                  dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                  dcterms:source <https://en.wikipedia.org/wiki/Earth%27s_inner_core> ;
                  skos:definition "Earth's inner core is a planetary layer within Earth's geosphere that is the inner most layer. It is primarily a solid ball with a radius of about 1,220 kilometres (760 miles). The inner core is believed to be composed of an iron–nickel alloy with some other elements. The temperature at the inner core's surface is estimated to be approximately 5700 K (5430 °C or 9806 °F)."@en ;
                  skos:prefLabel "inner core"@en .


###  http://sweetontology.net/realmEarthReference/Lithosphere
soreaer:Lithosphere rdf:type owl:NamedIndividual ,
                             soreag:GeosphereLayer ;
                    sorel:hasPart soreaer:crust ;
                    sorel:isPartOf sorea:EarthGeosphere ;
                    dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                    skos:prefLabel "lithosphere"@en .


###  http://sweetontology.net/realmEarthReference/LowerMantle
soreaer:LowerMantle rdf:type owl:NamedIndividual ,
                             soreag:GeosphereLayer ;
                    dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                    skos:prefLabel "lower mantle"@en .


###  http://sweetontology.net/realmEarthReference/Mantle
soreaer:Mantle rdf:type owl:NamedIndividual ,
                        soreag:GeosphereLayer ;
               sorel:hasPart soreaer:DLayer ,
                             soreaer:LowerMantle ,
                             soreaer:UpperMantle ;
               sorel:isPartOf sorea:EarthGeosphere ;
               dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
               dcterms:source <https://en.wikipedia.org/wiki/Earth%27s_mantle> ,
                              "Earth's mantle is a planetary layer within Earth's lithosphere that is its second outermost layer. The mantle lies underneith the crust, also within the lithosphere, with the boundary between the crust and mantle conventionally placed at the Mohorovičić discontinuity, a boundary defined by a contrast in seismic velocity. It consists of silicate rock and has a thickness of 2,900 kilometres."@en ;
               skos:prefLabel "mantle"@en .


###  http://sweetontology.net/realmEarthReference/MesopelagicLayer
soreaer:MesopelagicLayer rdf:type owl:NamedIndividual ,
                                  soreag:HydrosphereLayer ;
                         sorel:isPartOf soreaer:AphoticLayer ;
                         dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                         dcterms:source <https://en.wikipedia.org/wiki/Mesopelagic_zone> ;
                         skos:altLabel "mesopelagic zone"@en ;
                         skos:definition "The mesopelagic layer is a planetary layer of Earth's hydrosphere defined by light, and begins at the depth where only 1% of incident light reaches and ends where there is no light."@en ;
                         skos:prefLabel "mesopelagic layer"@en .


###  http://sweetontology.net/realmEarthReference/OuterCore
soreaer:OuterCore rdf:type owl:NamedIndividual ,
                           soreag:GeosphereLayer ;
                  dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                  dcterms:source <https://en.wikipedia.org/wiki/Earth%27s_outer_core> ;
                  skos:definition "Earth's inner core is a planetary layer within Earth's geosphere and is a fluid layer about 2,400 km (1,500 mi) thick and composed of mostly iron and nickel that lies above Earth's solid inner core and below its mantle."@en ;
                  skos:prefLabel "outer core"@en .


###  http://sweetontology.net/realmEarthReference/PelagicLayer
soreaer:PelagicLayer rdf:type owl:NamedIndividual ,
                              soreag:HydrosphereLayer ;
                     sorel:isPartOf soreaer:EarthHydrosphere ;
                     dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                     dcterms:source <https://en.wikipedia.org/wiki/Pelagic_zone> ;
                     skos:altLabel "pelagic zone"@en ;
                     skos:definition "The pelagic layer is a planetary layer of Earth's hydrosphere that consists of the water column of the open ocean."@en ;
                     skos:prefLabel "pelagic layer"@en .


###  http://sweetontology.net/realmEarthReference/PhoticLayer
soreaer:PhoticLayer rdf:type owl:NamedIndividual ,
                             soreag:HydrosphereLayer ;
                    sorel:hasPart soreaer:EpipelagicLayer ;
                    sorel:isPartOf soreaer:PelagicLayer ;
                    dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                    dcterms:source <https://en.wikipedia.org/wiki/Photic_zone> ;
                    skos:altLabel "photic zone"@en ;
                    skos:definition "The pelagic layer is a planetary layer of Earth's hydrosphere that is the uppermost layer of the ocean that receives sunlight enabling organisms within it to perform photosynthesis."@en ;
                    skos:prefLabel "photic layer"@en .


###  http://sweetontology.net/realmEarthReference/SkinLayer
soreaer:SkinLayer rdf:type owl:NamedIndividual ,
                           soreag:GeosphereLayer ;
                  dcterms:contributor <https://orcid.org/0000-0002-8742-7730> ;
                  skos:prefLabel "skin layer"@en .


###  http://sweetontology.net/realmEarthReference/Subsurface
soreaer:Subsurface rdf:type owl:NamedIndividual ;
                   skos:prefLabel "skin layer"@en .


###  http://sweetontology.net/realmEarthReference/TransitionalCrust
soreaer:TransitionalCrust rdf:type owl:NamedIndividual ,
                                   soreag:GeosphereLayer ;
                          sorel:hasPart soreaer:TransitionalLithosphere ;
                          dcterms:contributor <https://orcid.org/0000-0002-8742-7730> .


###  http://sweetontology.net/realmEarthReference/TransitionalLithosphere
soreaer:TransitionalLithosphere rdf:type owl:NamedIndividual ,
